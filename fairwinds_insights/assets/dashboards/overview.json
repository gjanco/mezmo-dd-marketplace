{
    "title": "Fairwinds Insights Overview",
    "description": "## Fairwinds Insights Overview\n\nThis dashboard will introduce you to the data being brought in from Fairwinds Insights. If no data appears please login to [Fairwinds Insights](https://insights.fairwinds.com), go to your Organization Settings page and enter your Datadog API key.",
    "author_info": {
        "author_name": "Fairwinds"
    },
    "widgets": [
      {
        "id": 0,
        "definition": {
          "type": "event_stream",
          "query": "fairwinds insights action item $severity.value $cluster.value $report.value",
          "tags_execution": "and",
          "event_size": "s",
          "title": "Recent Insights Events",
          "title_size": "16",
          "title_align": "left"
        },
        "layout": {
          "x": 66,
          "y": 6,
          "width": 36,
          "height": 59
        }
      },
      {
        "id": 1,
        "definition": {
          "type": "timeseries",
          "requests": [
            {
              "q": "sum:fairwinds.insights.action_items{$severity,$report} by {insights_cluster}",
              "display_type": "line",
              "style": {
                "palette": "dog_classic",
                "line_type": "solid",
                "line_width": "normal"
              }
            }
          ],
          "custom_links": [],
          "yaxis": {
            "label": "",
            "scale": "linear",
            "min": "auto",
            "max": "auto",
            "include_zero": true
          },
          "events": [
            {
              "q": "fairwinds insights action item  $severity.value $report.value",
              "tags_execution": "and"
            }
          ],
          "title": "Action Items by Cluster",
          "title_size": "16",
          "title_align": "left",
          "time": {
            "live_span": "1mo"
          },
          "show_legend": true
        },
        "layout": {
          "x": 0,
          "y": 6,
          "width": 66,
          "height": 19
        }
      },
      {
        "id": 2,
        "definition": {
          "type": "timeseries",
          "requests": [
            {
              "q": "sum:fairwinds.insights.action_items{$cluster,$report} by {severity}",
              "display_type": "line",
              "style": {
                "palette": "dog_classic",
                "line_type": "solid",
                "line_width": "normal"
              }
            }
          ],
          "custom_links": [],
          "yaxis": {
            "label": "",
            "scale": "linear",
            "min": "auto",
            "max": "auto",
            "include_zero": true
          },
          "events": [
            {
              "q": "fairwinds insights action item $cluster.value $report.value",
              "tags_execution": "and"
            }
          ],
          "title": "Action Items by Severity",
          "title_size": "16",
          "title_align": "left",
          "time": {
            "live_span": "1mo"
          },
          "show_legend": true
        },
        "layout": {
          "x": 0,
          "y": 25,
          "width": 66,
          "height": 22
        }
      },
      {
        "id": 3,
        "definition": {
          "type": "change",
          "requests": [
            {
              "q": "sum:fairwinds.insights.action_items{$severity,$cluster} by {insights_report}",
              "change_type": "absolute",
              "compare_to": "week_before",
              "increase_good": false,
              "order_by": "change",
              "order_dir": "desc"
            }
          ],
          "custom_links": [],
          "time": {
            "live_span": "1mo"
          }
        },
        "layout": {
          "x": 0,
          "y": 47,
          "width": 66,
          "height": 20
        }
      },
      {
        "id": 4,
        "definition": {
          "type": "query_table",
          "requests": [
            {
              "q": "sum:fairwinds.insights.action_items{*} by {insights_category, insights_cluster, insights_organization, insights_report, insights_title, severity, kube_namespace}",
              "aggregator": "avg",
              "limit": 50,
              "order": "desc"
            }
          ],
          "custom_links": [],
          "title": "Action Items Table",
          "title_size": "16",
          "title_align": "left"
        },
        "layout": {
          "x": 0,
          "y": 65,
          "width": 102,
          "height": 34
        }
      },
      {
        "id": 5,
        "definition": {
          "type": "query_value",
          "requests": [
            {
              "q": "sum:fairwinds.insights.action_items{insights_category:security}",
              "aggregator": "last"
            }
          ],
          "custom_links": [],
          "title": "Number of Security Action Items",
          "title_size": "16",
          "title_align": "left",
          "autoscale": true,
          "precision": 2
        },
        "layout": {
          "x": 0,
          "y": 0,
          "width": 25,
          "height": 6
        }
      },
      {
        "id": 6,
        "definition": {
          "type": "query_value",
          "requests": [
            {
              "q": "sum:fairwinds.insights.action_items{insights_category:reliability}",
              "aggregator": "last"
            }
          ],
          "custom_links": [],
          "title": "Number of Reliability Action Items",
          "title_size": "16",
          "title_align": "left",
          "autoscale": true,
          "precision": 2
        },
        "layout": {
          "x": 25,
          "y": 0,
          "width": 26,
          "height": 6
        }
      },
      {
        "id": 7,
        "definition": {
          "type": "query_value",
          "requests": [
            {
              "q": "sum:fairwinds.insights.action_items{insights_category:efficiency}",
              "aggregator": "last"
            }
          ],
          "custom_links": [],
          "title": "Number of Efficiency Action Items",
          "title_size": "16",
          "title_align": "left",
          "autoscale": true,
          "precision": 2
        },
        "layout": {
          "x": 51,
          "y": 0,
          "width": 26,
          "height": 6
        }
      },
      {
        "id": 8,
        "definition": {
          "type": "query_value",
          "requests": [
            {
              "q": "sum:fairwinds.insights.action_items{*}",
              "aggregator": "last",
              "conditional_formats": [
                {
                  "comparator": ">=",
                  "value": 0,
                  "palette": "white_on_gray"
                }
              ]
            }
          ],
          "custom_links": [],
          "title": "Number of Total Action Items",
          "title_size": "16",
          "title_align": "left",
          "autoscale": true,
          "precision": 0
        },
        "layout": {
          "x": 77,
          "y": 0,
          "width": 25,
          "height": 6
        }
      }
    ],
    "template_variables": [
      {
        "name": "severity",
        "default": "*",
        "prefix": "severity"
      },
      {
        "name": "cluster",
        "default": "*",
        "prefix": "insights_cluster"
      },
      {
        "name": "report",
        "default": "*",
        "prefix": "insights_report"
      }
    ],
    "layout_type": "free",
    "is_read_only": true,
    "notify_list": [],
