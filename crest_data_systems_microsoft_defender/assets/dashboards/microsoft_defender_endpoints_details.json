        {
            "id": 527965469422688,
            "layout": {
                "x": 32,
                "y": 0,
                "width": 156,
                "height": 34
            },
            "definition": {
                "title": "Endpoint Details",
                "title_size": "16",
                "title_align": "left",
                "requests": [
                    {
                        "query": {
                            "query_string": "source:crest_data_systems_microsoft_defender service:ms.defender.endpoint.endpoints @endpoint-name:$Endpoint-Name.value @endpoint-id:$Endpoint-Id.value",
                            "sort": {
                                "column": "@endpoint-seen-time",
                                "order": "desc"
                            },
                            "data_source": "logs_stream",
                            "indexes": []
                        },
                        "response_format": "event_list",
                        "columns": [
                            {
                                "field": "@lastSeen",
                                "width": "auto"
                            },
                            {
                                "field": "@endpoint-id",
                                "width": "auto"
                            },
                            {
                                "field": "@endpoint-name",
                                "width": "auto"
                            },
                            {
                                "field": "@endpoint-health-status",
                                "width": "auto"
                            },
                            {
                                "field": "@endpoint-risk",
                                "width": "auto"
                            },
                            {
                                "field": "@endpoint-seen-time",
                                "width": "auto"
                            }
                        ]
                    }
                ],
                "type": "list_stream"
            }
        },
        {
            "id": 2569899962339974,
            "layout": {
                "x": 109,
                "y": 37,
                "width": 44,
                "height": 36
            },
            "definition": {
                "title": "Endpoint Vulnerabilities",
                "title_size": "16",
                "title_align": "left",
                "type": "query_table",
                "requests": [
                    {
                        "formulas": [
                            {
                                "alias": "Count",
                                "limit": {
                                    "count": 900,
                                    "order": "desc"
                                },
                                "formula": "query1"
                            }
                        ],
                        "response_format": "scalar",
                        "queries": [
                            {
                                "search": {
                                    "query": "source:crest_data_systems_microsoft_defender service:ms.defender.endpoint.endpoint-software.vulns @endpoint-name:$Endpoint-Name.value @endpoint-id:$Endpoint-Id.value"
                                },
                                "data_source": "logs",
                                "compute": {
                                    "metric": "@vulnerability-id",
                                    "aggregation": "cardinality"
                                },
                                "name": "query1",
                                "indexes": [
                                    "*"
                                ],
                                "group_by": [
                                    {
                                        "facet": "@vuln-severity",
                                        "sort": {
                                            "metric": "@vulnerability-id",
                                            "aggregation": "cardinality",
                                            "order": "desc"
                                        },
                                        "limit": 30
                                    },
                                    {
                                        "facet": "@vulnerability-id",
                                        "sort": {
                                            "metric": "@vulnerability-id",
                                            "aggregation": "cardinality",
                                            "order": "desc"
                                        },
                                        "limit": 30
                                    }
                                ]
                            }
                        ]
                    }
                ]
            }
        },
        {
            "id": 150040825217076,
            "layout": {
                "x": 154,
