            "definition": {
                "title": "Volume Storage Details",
                "title_size": "16",
                "title_align": "left",
                "requests": [
                    {
                        "formulas": [
                            {
                                "formula": "query1",
                                "limit": {
                                    "count": 10,
                                    "order": "desc"
                                }
                            }
                        ],
                        "style": {
                            "palette": "datadog16"
                        },
                        "response_format": "scalar",
                        "queries": [
                            {
                                "query": "avg:cds.netapp.aiqum.volume_storage_details.space{$AIQUM_Instance} by {name,size-type}",
                                "data_source": "metrics",
                                "name": "query1",
                                "aggregator": "last"
                            }
                        ]
                    }
                ],
                "type": "sunburst",
                "hide_total": false,
                "legend": {
                    "type": "automatic"
                }
            }
