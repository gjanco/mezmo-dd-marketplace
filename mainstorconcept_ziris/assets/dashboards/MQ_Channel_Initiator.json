      "id": 810819877458390,
      "definition": {
        "title": "Channel Availability",
        "type": "group",
        "background_color": "vivid_blue",
        "show_title": true,
        "layout_type": "ordered",
        "widgets": [
          {
            "id": 8427567827454431,
            "definition": {
              "type": "note",
              "content": "Remaining channels are calculated by the difference between the maximum available channels and the peak channel usage.",
              "background_color": "white",
              "font_size": "18",
              "text_align": "center",
              "vertical_align": "center",
              "show_tick": true,
              "tick_pos": "50%",
              "tick_edge": "bottom",
              "has_padding": true
            },
            "layout": {
              "x": 0,
              "y": 0,
              "width": 8,
              "height": 1
            }
          },
          {
            "id": 122953544192772,
            "definition": {
              "title": "Remaining Current Channels",
              "title_size": "16",
              "title_align": "left",
              "show_legend": true,
              "legend_layout": "horizontal",
              "legend_columns": [
                "avg",
                "min",
                "max",
                "value",
                "sum"
              ],
              "type": "timeseries",
              "requests": [
                {
                  "formulas": [
                    {
                      "alias": "Current Channels Remaining",
                      "formula": "round(query2 - query1)"
                    }
                  ],
                  "response_format": "timeseries",
                  "queries": [
                    {
                      "query": "avg:mainstorconcept.zos.mq.channel.current.max{$qm}",
                      "data_source": "metrics",
                      "name": "query2"
                    },
                    {
                      "query": "avg:mainstorconcept.zos.mq.channel.current.peak{$qm}",
                      "data_source": "metrics",
                      "name": "query1"
                    }
                  ],
                  "style": {
                    "palette": "dog_classic",
                    "line_type": "solid",
                    "line_width": "normal"
                  },
                  "display_type": "line"
                }
              ],
              "markers": [
                {
                  "label": "Low Channel Availability",
                  "value": "15 < y < 30",
                  "display_type": "warning solid"
                },
                {
                  "label": "Critical Channel Availability",
                  "value": "0 < y < 15",
                  "display_type": "error dashed"
                }
              ]
            },
            "layout": {
              "x": 0,
              "y": 1,
              "width": 4,
              "height": 3
            }
          },
