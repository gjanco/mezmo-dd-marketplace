              "response_format": "scalar",
              "queries": [
                {
                  "query": "sum:rapdev.zoom.meetings.participants{$zoom_account_name}",
                  "data_source": "metrics",
                  "name": "query1",
                  "aggregator": "last"
                }
              ]
            }
          ],
          "autoscale": false,
          "precision": 0
        }
      },
