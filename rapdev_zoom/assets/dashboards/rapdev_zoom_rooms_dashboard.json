{
    "author_name": "RapDev",
    "title": "RapDev Zoom Rooms Dashboard",
    "description": "A dashboard of all Zoom rooms and their status.\n\nClone this template dashboard to make changes and add your own graph widgets.",
    "widgets": [
      {
        "id": 0,
        "layout": {
          "x": 0,
          "y": 1,
          "width": 19,
          "height": 11
        },
        "definition": {
          "type": "image",
          "url": "https://cuit.columbia.edu/sites/default/files/styles/cu_crop/public/content/zoom-logo-transparent-6.png?itok=PJk3QEss",
          "sizing": "fit"
        }
      },
      {
        "id": 12,
        "layout": {
          "x": 20,
          "y": 21,
          "width": 19,
          "height": 10
        },
        "definition": {
          "title": "Available Rooms (Status)",
          "title_size": "16",
          "title_align": "left",
          "type": "query_value",
          "requests": [
            {
              "q": "max:rapdev.zoom.room.status.count{zoom_room_status:available,$zoom_account_name}",
              "aggregator": "last",
              "conditional_formats": [
                {
                  "comparator": ">=",
                  "palette": "white_on_green",
                  "value": 1
                },
                {
                  "custom_bg_color": "#0179f9",
                  "comparator": "<",
                  "palette": "custom_bg",
                  "value": 1
                }
              ]
            }
          ],
          "autoscale": true,
          "precision": 0
        }
      },
      {
        "id": 14,
        "layout": {
          "x": 20,
          "y": 32,
          "width": 19,
          "height": 11
        },
        "definition": {
          "title": "Rooms Under Construction (Status)",
          "title_size": "16",
          "title_align": "left",
          "type": "query_value",
          "requests": [
            {
              "q": "sum:rapdev.zoom.room.status.count{zoom_room_status:under_construction,$zoom_account_name}",
              "aggregator": "last",
              "conditional_formats": [
                {
                  "comparator": ">=",
                  "palette": "white_on_red",
                  "value": 1
                },
                {
                  "comparator": ">=",
                  "palette": "white_on_green",
                  "value": 0
                }
              ]
            }
          ],
          "precision": 0
        }
      },
      {
        "id": 15,
        "layout": {
          "x": 20,
          "y": 9,
          "width": 19,
          "height": 11
        },
        "definition": {
          "title": "Rooms Offline (Status)",
          "title_size": "16",
          "title_align": "left",
          "type": "query_value",
          "requests": [
            {
              "q": "sum:rapdev.zoom.room.status.count{zoom_room_status:offline,$zoom_account_name}.rollup(avg)",
              "aggregator": "last",
              "conditional_formats": [
                {
                  "comparator": ">",
                  "palette": "white_on_red",
                  "value": 0
                },
                {
                  "comparator": "<",
                  "palette": "white_on_green",
                  "value": 1
                }
              ]
            }
          ],
          "autoscale": true,
          "precision": 0
        }
      },
      {
        "id": 5115189288791516,
        "layout": {
          "x": 0,
          "y": 29,
          "width": 19,
          "height": 14
        },
        "definition": {
          "title": "Participants in Meetings",
          "title_size": "16",
          "title_align": "left",
          "type": "query_value",
          "requests": [
            {
              "formulas": [
                {
                  "formula": "default_zero(query1)"
