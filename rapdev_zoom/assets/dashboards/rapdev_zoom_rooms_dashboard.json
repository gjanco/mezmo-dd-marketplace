{
    "author_name": "RapDev",
    "description": "Dashboard of all Zoom rooms and their status\n",
    "layout_type": "free",
    "template_variables": [
        {
            "default": "*",
            "name": "room_name",
            "prefix": "zoom_room_name"
        },
        {
            "default": "*",
            "name": "zoom_account",
            "prefix": "zoom_account_name"
        }
    ],
    "title": "RapDev Zoom Rooms Dashboard",
    "widgets": [
        {
            "definition": {
                "sizing": "fit",
                "type": "image",
                "url": "https://cuit.columbia.edu/sites/default/files/styles/cu_crop/public/content/zoom-logo-transparent-6.png?itok=PJk3QEss"
            },
            "id": 0,
            "layout": {
                "height": 11,
                "width": 19,
                "x": 0,
                "y": 1
            }
        },
        {
            "definition": {
                "autoscale": true,
                "precision": 0,
                "requests": [
                    {
                        "aggregator": "last",
                        "conditional_formats": [
                            {
                                "comparator": ">=",
                                "palette": "white_on_green",
                                "value": 1
                            },
                            {
                                "comparator": "<",
                                "custom_bg_color": "#0179f9",
                                "palette": "custom_bg",
                                "value": 1
                            }
                        ],
                        "q": "max:rapdev.zoom.room.status.count{$zoom_account,zoom_room_status:available}"
                    }
                ],
                "title": "Available Rooms (Status)",
                "title_align": "left",
                "title_size": "16",
                "type": "query_value"
            },
            "id": 12,
            "layout": {
                "height": 10,
                "width": 19,
                "x": 20,
                "y": 21
            }
        },
        {
            "definition": {
                "precision": 0,
                "requests": [
                    {
                        "aggregator": "last",
                        "conditional_formats": [
                            {
                                "comparator": ">=",
                                "palette": "white_on_red",
                                "value": 1
                            },
                            {
                                "comparator": ">=",
                                "palette": "white_on_green",
                                "value": 0
                            }
                        ],
                        "q": "sum:rapdev.zoom.room.status.count{$zoom_account,zoom_room_status:under_construction}"
