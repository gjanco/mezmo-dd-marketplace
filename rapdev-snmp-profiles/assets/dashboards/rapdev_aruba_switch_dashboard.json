      "definition": {
        "custom_links": [],
        "title_size": "16",
        "title": "Memory Usage (%)",
        "title_align": "left",
        "custom_unit": "%",
        "precision": 2,
        "autoscale": false,
        "requests": [
          {
            "q": "(avg:snmp.Memory.Total{$IP,$SNMP_Profile,$Host, $Sysname}-avg:snmp.Memory.Free{$IP,$SNMP_Profile,$Host, $Sysname})/avg:snmp.Memory.Total{$IP,$SNMP_Profile,$Host, $Sysname}*100",
            "aggregator": "last",
            "conditional_formats": [
              {
                "palette": "white_on_red",
                "comparator": ">",
                "value": 90
              },
              {
                "palette": "white_on_yellow",
                "comparator": ">=",
                "value": 70
              },
              {
                "palette": "white_on_green",
                "comparator": "<",
                "value": 70
              }
            ]
          }
        ],
        "type": "query_value"
      },
      "layout": {
        "y": 0,
        "x": 55,
        "width": 21,
        "height": 15
      },
      "id": 8091321264724602
    },
