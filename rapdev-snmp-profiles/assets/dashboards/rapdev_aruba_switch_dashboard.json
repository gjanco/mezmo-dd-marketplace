      {
         "id":8091321264724602,
         "layout":{
            "x":55,
            "y":0,
            "width":21,
            "height":15
         },
         "definition":{
            "title":"Memory Usage (%)",
            "title_size":"16",
            "title_align":"left",
            "type":"query_value",
            "requests":[
               {
                  "q":"(avg:snmp.Memory.Total{$IP,$SNMP_Profile,$Host}-avg:snmp.Memory.Free{$IP,$SNMP_Profile,$Host})/avg:snmp.Memory.Total{$IP,$SNMP_Profile,$Host}*100",
                  "aggregator":"last",
                  "conditional_formats":[
                     {
                        "comparator":">",
                        "palette":"white_on_red",
                        "value":90
                     },
                     {
                        "comparator":">=",
                        "palette":"white_on_yellow",
                        "value":70
                     },
                     {
                        "comparator":"<",
                        "palette":"white_on_green",
                        "value":70
                     }
                  ]
               }
            ],
            "autoscale":false,
            "custom_unit":"%",
            "custom_links":[
               
            ],
            "precision":2
         }
      },
