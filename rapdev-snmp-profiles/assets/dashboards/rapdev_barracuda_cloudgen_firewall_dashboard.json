        "custom_unit": "%",
        "precision": 2,
        "requests": [
          {
            "q": "avg:snmp.CPU.Utilization{$Host,$IP,$SNMP_Profile, $Sysname}",
            "aggregator": "avg",
            "conditional_formats": [
              {
                "palette": "white_on_green",
                "comparator": "<",
                "value": 70
