                                        "value": 1.5
                                    },
                                    {
                                        "comparator": ">=",
                                        "palette": "white_on_green",
                                        "value": 1
                                    }
                                ],
                                "formula": "query2",
                                "limit": {
                                    "count": 500,
                                    "order": "desc"
                                }
                            }
                        ],
                        "queries": [
                            {
                                "aggregator": "last",
                                "data_source": "metrics",
                                "name": "query1",
                                "query": "avg:snmp.ifAdminStatus{$snmp_profile,*,$interface,$snmp_profile,$snmp_host} by {interface}"
                            },
                            {
                                "aggregator": "last",
                                "data_source": "metrics",
                                "name": "query2",
                                "query": "avg:snmp.ifOperStatus{$snmp_profile,*,$interface,$snmp_profile,$snmp_host} by {interface}"
                            }
