        {
            "definition": {
                "has_search_bar": "auto",
                "requests": [
                    {
                        "formulas": [
                            {
                                "alias": "Admin Status",
                                "cell_display_mode": "bar",
                                "conditional_formats": [
                                    {
                                        "comparator": "<",
                                        "palette": "white_on_red",
                                        "value": 1
                                    },
                                    {
                                        "comparator": ">",
                                        "palette": "white_on_red",
                                        "value": 1.5
                                    },
                                    {
                                        "comparator": ">=",
                                        "palette": "white_on_green",
                                        "value": 1
                                    }
                                ],
                                "formula": "query1"
                            },
                            {
                                "alias": "Oper Status",
