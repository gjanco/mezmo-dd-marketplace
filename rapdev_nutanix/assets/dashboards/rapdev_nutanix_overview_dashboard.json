      "definition": {
        "title": "Days Until License Expiry ",
        "title_size": "16",
        "title_align": "left",
        "type": "query_value",
        "requests": [
          {
            "q": "avg:rapdev.nutanix.license.days_left{$nutanix_cvm,$nutanix_cluster}",
            "aggregator": "last",
            "conditional_formats": [
              {
                "comparator": ">=",
                "palette": "white_on_green",
                "value": 60
              },
              {
                "comparator": "<",
                "palette": "white_on_red",
                "value": 60
              }
            ]
          }
        ],
        "autoscale": false,
        "custom_unit": "Days",
        "precision": 0
