              "bar"
            ],
            "aggregator": "last"
          },
          {
            "q": "avg:rapdev.nutanix.controller_io_bandwidth_kBps{nutanix_type:vm,$nutanix_cluster,$host,$hypervisor,$vm_description,$nutanix_cvm} by {host, vm_description, nutanix_host, hypervisor}",
