            "q": "avg:rapdev.nutanix.hypervisor_cpu_usage{nutanix_type:vm,$nutanix_cluster,$host,$hypervisor,$vm_description,$nutanix_cvm} by {host, vm_description, nutanix_host, hypervisor}",
